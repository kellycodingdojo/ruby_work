<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Action</th>
	</tr>
	<% @users.each do |user|%>
	<tr>
		<td><%= user.first_name %></td>
		<td><%= user.last_name %></td>
		<td><%= user.email %></td>
		<td>
		<%= link_to "Show", user %> | 
		<%= link_to "Edit", edit_user_path(user) %> | 
		<%= link_to "Delete", user, data: {method: "DELETE", confirm: "Are you sure you want to delete this?"} %></td>
	</tr>
	<% end %>
</table>

<%= link_to "New User", new_user_path %>

